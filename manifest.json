{
    "manifest_version": 2,
    "name": "insta-viewer",
    "version": "1.0",
    "description": "View Instagram Accounts Without Having An Account",
    "icons": {
        "48": "icons/icon-48.png",
        "96": "icons/icon-96.png"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "https://www.instagram.com/"
    ],
    "background": {
        "scripts": [
            "insta-viewer.js"
        ]
    }
}